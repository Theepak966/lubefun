APP_ENV=production
APP_PORT=3000
SESSION_SECRET=dev-session-secret-change-me

APP_NAME=Lubee
APP_ABBREVIATION=LUB
APP_URL=http://localhost:8080
APP_PUBLIC_URL=http://localhost:8080
APP_PUBLIC_SECURE=false
APP_LISTEN_SECURE=false

# DB settings are required by the app at startup. If credentials are wrong,
# some features will error, but we can still validate nginx + websocket upgrade.
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=lubee_app
DB_USERNAME=lubee
DB_PASSWORD=lubee_pass
